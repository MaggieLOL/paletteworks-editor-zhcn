  "version": "1.9.7",
  "scripts": {
    "dev": "svelte-kit dev",
    "clean": "rimraf build",
    "build:svelte-kit": "svelte-kit build",
    "build": "run-s clean build:svelte-kit",
    "preview": "svelte-kit preview",
    "test": "jest",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "eslint --ignore-path .gitignore src",
    "typesafe-i18n": "typesafe-i18n",
    "version": "@rstacruz/bump-cli",
    "version:patch": "@rstacruz/bump-cli -p",
    "version:minor": "@rstacruz/bump-cli -m",
    "version:major": "@rstacruz/bump-cli -M",
    "version:v": "@rstacruz/bump-cli -v $npm_config_v",
    "beforeRelease": "run-s check lint test",
    "tag": "git tag v$npm_package_version && git push origin v$npm_package_version",
    "release": "run-s beforeRelease version tag",
    "release:patch": "run-s beforeRelease version:patch tag",
    "release:minor": "run-s beforeRelease version:minor tag",
    "release:major": "run-s beforeRelease version:major tag"
  },
  "license": "MIT",
