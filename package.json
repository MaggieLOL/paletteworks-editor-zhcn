    "@types/throttle-debounce": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "@vercel/node": "^1.13.0",
    "@zerodevx/svelte-toast": "^0.7.0",
    "attr-accept": "^2.2.2",
    "autoprefixer": "^10.4.2",
    "bpm-detective": "^2.0.5",
    "core-js": "^3.21.1",
    "dexie": "^3.2.1",
